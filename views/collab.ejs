<% layout("boilerplate.ejs") %>
<div class="row mx-5">
  <% for(collab of collabs){ %>
  <div class="card my-3">
    <div class="row">
      <div class="col-md-4">
        <img src="<%= collab.images[0].url %>" class="img-fluid" alt="" />
      </div>
      <div class="col-md-8">
        <div class="card-body">
          <h5 class="card-title"><%= collab.title %></h5>
          <p class="card-text"><%= collab.description %></p>
          <p class="text-muted"><%= collab.skills %></p>
          <a href="/collab/<%= collab._id %>" class="btn btn-primary"
            >View <%= collab.title %></a
          >
        </div>
      </div>
    </div>
  </div>
  <% } %>
</div>
